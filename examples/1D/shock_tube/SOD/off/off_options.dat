<?xml version="1.0"?>
<OFF_Options>
  <inputs>
    <path>              ./input/    </path>
    <solver_file_name>  solver.opt  </solver_file_name>
    <mesh_file_name>    sod.geo     </mesh_file_name>
    <bc_file_name>      sod.bco     </bc_file_name>
    <init_file_name>    sod.itc     </init_file_name>
    <procmap_file_name> procmap.opt </procmap_file_name>
  </inputs>
  <outputs>
    <path>                       ./output/ </path>
    <solution_file_name>         sod.sol   </solution_file_name>
    <profiling_file_name>        profile   </profiling_file_name>
    <shell_out_frequency>        10        </shell_out_frequency>
    <solution_out_frequency>     10        </solution_out_frequency>
    <solution_restart_frequency> 10        </solution_restart_frequency>
    <probe_out_frequency>        1         </probe_out_frequency>
  </outputs>
</OFF_Options>
